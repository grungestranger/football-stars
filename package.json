        "sass-loader": "^7.3.1",
        "vue-template-compiler": "^2.6.11"
    },
    "dependencies": {
        "dotenv": "^6.2.0",
        "express": "^4.18.2",
        "fs": "0.0.1-security",
        "http": "0.0.0",
        "https": "^1.0.0",
